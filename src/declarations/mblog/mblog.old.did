type User = 
 record {
   id: principal;
   name: opt text;
 };
type Time = int;
type Message = 
 record {
   author: text;
   "text": text;
   time: Time;
 };
service : {
  follow: (principal) -> ();
  follows: () -> (vec principal) query;
  get_follow_data: () -> (vec User);
  get_name: () -> (text) query;
  post: (text, text) -> ();
  posts: (Time) -> (vec Message) query;
  postsby: (Time, principal) -> (vec Message);
  set_name: (text) -> ();
  timeline: (Time) -> (vec Message);
}
